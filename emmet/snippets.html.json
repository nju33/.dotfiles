{
  "html": {
		"abbreviations": {
      "oghead": "<head prefix=\"og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#\"/>",
      "ogtitle": "<meta property=\"og:title\" content=\"${1:title}\"/>",
      "ogdesc": "<meta property=\"og:description\" content=\"${1:desc}\"/>",
      "ogimage": "<meta property=\"og:image\" content=\"${1:imgurl}\"/>",
      "ogtype": "<meta property=\"og:type\" content=\"${1:type}\"/>",
      "ogurl": "<meta property=\"og:url\" contetnt=\"${1:url}\"/>",

      "twitter": "a[href=http://twitter.com/intent/tweet?text=${1:text}&url=${2:url}&via=${3:account}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'tweetwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "facebook": "a[href=http://www.facebook.com/sharer.php?u=${1:url}&t=${2:title}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'fbwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "gplus": "a[href=https://plus.google.com/share?url=${1:url}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'g+window','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "hatebu": "a[href=http://b.hatena.ne.jp/add?mode=confirm&url=${1:url}&title=${2:title}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'hatebuwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "pocket": "a[href=http://getpocket.com/edit?url=${1:url}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'pockettwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "tumblr": "a[href=http://www.tumblr.com/share/link?url=${1:url}&name=${2:title}&description=${3:desc}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'tumblrtwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "pinterest": "a[href=//jp.pinterest.com/pin/create/button/?url=${1:url}&media=${2:imgurl}&description=${3:desc}][target=_blank][onclick=window.open(encodeURI(decodeURI(this.href)),'pinterestwindow','width=550,height=450,personalbar=0,toolbar=0,scrollbars=1,resizable=1');return false;]",
      "line": "a[href=http://line.me/R/msg/text/?${1:text}${2:url}]{LINEで送る}"
    }
  }
}
