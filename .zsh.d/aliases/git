alias @st='git status'
alias @d='git diff'
alias @dc='git diff --cached'
alias @b='git branch'
alias @bd='git branch -D'
alias @bm='git branch -m'
alias @a='git add'
alias @ap='git add --patch'
alias @aa='git add --all'
alias @cm='git commit'
alias @cma='git commit -am'
alias @cmsg='git commit -m'
alias @co='git checkout'
alias @cp='git checkout --patch'
alias @cb='git checkout -b'
alias @lg='git log --oneline --stat --graph --max-count 10'
alias @log='git log --oneline --stat --graph'
alias @rlg="git reflog"
alias @mm='git merge master'
alias @p='git pull origin $(current_branch)'
alias @@='git push origin $(current_branch)'
